<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DeliveryApp"
                   namespace="DeliveryApp.Entity">

  <class name="Distance" table="[distance]" lazy="false">
    <id name="Id" column="id" type="int">
      <generator class="native" />
    </id>
    <property name="CityFromId" column="city_from_id" type="int" />
    <property name="CityToId" column="city_to_id" type="int" />
    <property name="DistanceData" column="distance" type="int" />
  </class>
</hibernate-mapping>